{
  "name": "nativekits",
  "version": "1.0.2",
  "description": "A beautiful React Native UI component library with TypeScript support",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "lib",
    "src",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "prepare": "npm run build",
    "lint": "eslint"
  },
  "keywords": [
    "react-native",
    "ui",
    "components",
    "typescript",
    "accordion",
    "expo",
    "cli"
  ],
  "author": "thund3rhawk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thund3rhawk/nativekits.git"
  },
  "bugs": {
    "url": "https://github.com/thund3rhawk/nativekits/issues"
  },
  "homepage": "https://nativekits.vercel.app",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^30.0.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.8",
    "@types/react": "18.2.0",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "metro-react-native-babel-preset": "^0.77.0",
    "react": "18.2.0",
    "react-native": "0.72.8",
    "react-native-gesture-handler": "^2.14.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3"
